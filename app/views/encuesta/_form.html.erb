<style type="text/css">
  .hidden {
  display: none !important;}
</style>

<br>

  <%= form_for @encuestum do |f| %>
    <br>
    <% if @encuestum.errors.any? %>
    <div class="container col-md-10">
      <div class="alert alert-danger" role="alert">
        <h2><%= pluralize(@encuestum.errors.count, "errores") %> impiden que se guarde esta encuesta:</h2>
        <ul>
          <% @encuestum.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    </div>
  <% end %>
